<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nubia.filltoolset"
    android:versionCode="1"
    android:versionName="3.2.0.2" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />
    <!--  发送消息-->
    <uses-permission android:name="android.permission.SEND_SMS"/>
    <!--  阅读消息-->
    <uses-permission android:name="android.permission.READ_SMS"/>
    <!--  写入消息-->
    <uses-permission android:name="android.permission.WRITE_SMS" />
    <!-- 接收消息 -->
    <uses-permission android:name="android.permission.RECEIVE_SMS" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.WRITE_CONTACTS" /> 
    <uses-permission android:name="android.permission.KILL_BACKGROUND_PROCESSES"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS"/>
    <application
        android:allowBackup="true"
        android:icon="@drawable/tab_com_selected"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.nubia.filltoolset.main.MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.nubia.filltoolset.messagefill.MessageFillActivity"
            android:screenOrientation="portrait" >
        </activity> 
        <activity
            android:name="com.nubia.filltoolset.contactsfill.ContactsFillActivity"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.nubia.filltoolset.memmoryfill.MemmoryFillActivity"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.nubia.filltoolset.audioandvideotest.AudioAndVideoTestActivity"
            android:screenOrientation="portrait" >
        </activity>
                <activity
            android:name="com.nubia.filltoolset.niuduncleartest.NiuduncleartestActivity"
            android:screenOrientation="portrait" >
        </activity>
                        <activity
            android:name="com.nubia.filltoolset.mmsfill.MmsFillActicity"
            android:screenOrientation="portrait" >
        </activity>
    </application>

</manifest>
